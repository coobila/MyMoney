            android:name=".MyMoneyZeroActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
        android:label="AddOutData"
        android:name="Addout"/>
        <activity
            android:label="AddInData"
            android:name="Addin"/>
